generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Feeder {
  id  String @id @default(uuid())
  Vessel String
  Voyage String
  ETD String
  ETA String
  POL String
  POD String
}

model Track {
  id  String @id @default(uuid())
  POL String
  POD String
}
